  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@vitest/ui": "^0.5.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vitest": "^0.5.5"
  },
  "dependencies": {},
  "peerDependencies": {},
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
